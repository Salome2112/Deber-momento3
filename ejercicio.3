
'use strict'
function numbers(nums) {
    for (let i = 0; i < nums.length; i++) {
        let number = '';
        for (let j = i + 1; j < nums.length; j++) {
            number+= nums[i] + '' + nums[j] + ' ';
        }
        if (number !== '') {
            alert(number.trim());
        }
    }
}
